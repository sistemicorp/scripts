     "6":  {"mode": "DAC",    "port": "9"},
     "8":  {"mode": "DAC",    "port": "5"},
     "9":  {"mode": "DAC",    "port": "6"},
     "10": {"mode": "GPO",    "port": "4"},
     "13": {"mode": "GPI",    "port": "0"},
