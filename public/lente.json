{
  // turn on demo mode.  Creates test user accounts, ...
  "demo": true,

  // Result JSON files be stored as encrypted, true (default) or false,
  // If the results were not encrypted by Prism, they won't be ecrypted by Lente
  "results_bkup_encrypted": false,

  // enter IP Address:port for upstream server, example "http://35.123.432.190:6595"
  // else use null to disable upstream sending
  "result_server_url": null,

  "postgres": {
    "resultbasekeysv1": {
      "user": "postgres",
      // !! Change "pw" to a real password for a real deployment
      // !! This pw must match your postgres deployment too
      "pw": "qwerty",

      // ip address of the postgres database, use `127.0.0.1` if locahost
      "ip": "127.0.0.1"
    }
  }
}
